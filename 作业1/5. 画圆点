import matplotlib.pyplot as plt    #导入库

h, v0, g = 3000, 200, 9.8          #设参数
t=eval(input())                    #读取t数值

xt=v0*t                            #算横坐标
yt=h-1/2*g*t**2                    #算纵坐标

plt.plot(xt,yt,'ro')               #画红点
plt.grid('on')                     #开网格
plt.axis([0,5000,0,3000])          #轴数范围
plt.show()